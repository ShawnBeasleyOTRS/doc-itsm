msgstr ""

#. type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/content/incident-problem.xml:33
msgid "SLA"
msgstr ""

#. type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/content/incident-problem.xml:38
msgid "impact"
msgstr ""

#. type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/content/incident-problem.xml:43
msgid "priority"
